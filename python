cartItems=[]
cartTotal=0

def addItem(itemName,itemPrice):
    cartItems.append(itemName)
    return itemPrice
    
cartTotal+=addItem('Apple',5)
cartTotal+=addItem('Orange',4)
cartTotal+=addItem('Apple',6)
cartTotal+=addItem('Orange',7)


cartSummary=dict((item,cartItems.count(item)) for item in cartItems)
print(cartSummary)
print(cartItems)
print(cartTotal)






        
        
        
    
        
        
